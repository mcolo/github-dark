{
    "name": "Github Dark",
    "version": "0.1",
    "description": "Display code on github with a dark theme",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.github.com/*", "http://*.github.com/*"],
            "css": ["styles.css"],
            "js": ["toggle.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
